addCommand("stat", func(player, args) {
	if len(args) < 2 {
		player.SendPacket(packets.ServerMessage("Invalid args.  Usage: ::stat <idx> <lvl>"))
		return
	}

	idx = toInt(args[0])
	lvl = toInt(args[1])
	player.Skillset.SetCur(idx, lvl)
	player.Skillset.SetMax(idx, lvl)
	player.SendPacket(packets.PlayerStats(player))
})