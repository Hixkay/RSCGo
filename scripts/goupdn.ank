addCommand("goup", func(player, args) {
	nextLocation = player.Above()
	player.Teleport(nextLocation.CurX(), nextLocation.CurY())
	player.SendPacket(packets.PlaneInfo(player))
})
addCommand("godown", func(player, args) {
	nextLocation = player.Below()
	player.Teleport(nextLocation.CurX(), nextLocation.CurY())
	player.SendPacket(packets.PlaneInfo(player))
})