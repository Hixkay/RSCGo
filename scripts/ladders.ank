strings = import("strings")

func objectAction() {
    cmd = objectDefs[object.ID].Commands[0]
    if cmd == "climb-down" || cmd == "climb-up" || cmd == "go-up" {
        if strings.HasSuffix(cmd, "up") {
            player.SetLocation(player.Above(), true)
        } else {
            player.SetLocation(player.Below(), true)
        }
        client.UpdatePlane()
        return true
    }
    return false
}
