addCommand("dobj", func(player, args) {
	if len(args) == 0 {
		args = [toString(player.CurX()), toString(player.CurY())]
	}
	if len(args) < 2 {
		player.SendPacket(packets.ServerMessage("Invalid args.  Usage: ::dobj <x> <y>"))
		return
	}
	x = toInt(args[0])
	y = toInt(args[1])
	object = getObjectAt(x, y)
	if object == nil {
		player.SendPacket(packets.ServerMessage("Can not find object."))
		return
	}

	log.cmdf("%v deleted object %v", player, object)
	removeObject(object)
})