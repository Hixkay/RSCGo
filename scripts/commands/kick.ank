strings = import("strings")

addCommand("kick", func(player, args) {
  	if len(args) < 1 {
  		player.Message("Invalid args.  Usage: ::kick <username>")
  		return
  	}
	target, ok = getPlayerByName(base37(strings.TrimSpace(strings.Join(args, " "))))
	if target == nil || !ok {
  		player.Message("Could not find player.")
		return
	}
	player.Message("Kicking: '" + target.Username + "'")
	kick(target)
})
